# compton
compton --config ~/.config/compton.conf -f --vsync opengl &

# set the gamma
xgamma -gamma 0.85 &

# fonts
bash ~/.scripts/fonts.sh &

# touchpad
bash ~/.scripts/touchpad.sh &

# beep off
xset -b &

# urxvt daemon
urxvtd &

# set the background
./.fehbg &

# bar
sleep 2s && ./.scripts/bar.sh &

# start mpd
mpd &

# clipmenu
clipmenud &

# mpd scrobbler
mpdas &