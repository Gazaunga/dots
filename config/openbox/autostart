# nm
NetworkManager &

# compton
compton --config ~/.config/compton.conf -f --vsync opengl &

# set the gamma
xgamma -gamma 0.85 &

# fonts
bash ~/.scripts/fonts.sh &

# touchpad
bash ~/.scripts/touchpad.sh &

# volume icon
volumeicon &

# network manager applet
nm-applet &

# beep off
xset -b &

# urxvt daemon
urxvtd &

# set the background
./.fehbg &

# megasync
megasync &

# bar
sleep 3s && ./.scripts/bar.sh &

# start mpd
mpd &

# clipmenu
clipmenud &